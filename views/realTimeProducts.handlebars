<h2>Add product</h2>
<form id="form">
    <label for="title">Title</label><input type="text" id="title" name="title"/>
    <label for="price">Price</label><input type="number" id="price" name="price"/>
    <label for="description">Description</label><input type="text" id="description" name="description"/>
    <label for="code">Code</label><input type="number" id="code" name="code"/>
    <label for="stock">Stock</label><input type="number" id="stock" name="stock"/>
    <input type="submit" value="Send"/>
</form>

<h2>Delete product</h2>
<form id="deleteForm">
    <label for="deleteId">ID to delete</label><input type="text" id="deleteId" name="deleteId"/>
    <input type="submit" value="Delete"/>
</form>

<p id="products"></p>

